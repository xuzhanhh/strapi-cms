{
  "connection": "default",
  "collectionName": "sections",
  "info": {
    "name": "section"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "string",
      "default": " ",
      "required": true
    },
    "articles": {
      "via": "section",
      "collection": "article"
    }
  }
}
